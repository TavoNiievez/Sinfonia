{
    "name": "tavoniievez/sinfonia",
    "description": "sinfonia web app",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.11",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^2.5",
        "doctrine/doctrine-bundle": "^2.1",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/flex": "^1.10",
        "symfony/framework-bundle": "5.1.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/yaml": "5.1.*"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3",
        "codeception/module-doctrine2": "^1.0",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-rest": "^1.2",
        "codeception/module-symfony": "^1.2",
        "codeception/verify": "^2.1",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpmd/phpmd": "^2.9",
        "rector/rector": "^0.8",
        "roave/security-advisories": "dev-latest",
        "sensiolabs/security-checker": "^6.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/debug-bundle": "^5.1",
        "symfony/maker-bundle": "^1.23",
        "symfony/twig-bundle": "5.1.*",
        "symfony/var-dumper": "5.1.*",
        "symfony/web-profiler-bundle": "5.1.*",
        "vimeo/psalm": "^4.1",
        "zenstruck/foundry": "^1.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.4.11"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [ "tests/_build/support" ],
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "ci": [
            "@composer validate --strict",
            "@test",
            "@psalm"
        ],
        "test": "vendor/bin/codecept run --colors",
        "psalm": "vendor/bin/psalm --no-cache",
        "rector": "vendor/bin/rector process src -c rector.php --no-progress-bar -n",
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
